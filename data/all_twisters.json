    "twisters": [
      {
        "id": 1,
        "text": "Шла Саша по шоссе и сосала сушку.",
        "difficulty": "easy",
        "tags": ["классика", "дети"],
        "length": 35
      },
      {
        "id": 2,
        "text": "Карл у Клары украл кораллы, а Клара у Карла украла кларнет.",
        "difficulty": "medium",
        "tags": ["классика", "кража"],
        "length": 54
      },
      {
        "id": 3,
        "text": "На дворе трава, на траве дрова, не руби дрова на траве двора.",
        "difficulty": "easy",
        "tags": ["природа"],
        "length": 58
      },
      {
        "id": 4,
        "text": "Четыре чёрненьких чумазеньких чертёнка чертили чёрными чернилами чертёж.",
        "difficulty": "hard",
        "tags": ["цвета", "демоны"],
        "length": 72
      },
      {
        "id": 5,
        "text": "Сшит колпак не по-колпаковски, переколпаковать бы колпак.",
        "difficulty": "medium",
        "tags": ["одежда"],
        "length": 52
      },
      {
        "id": 6,
        "text": "Бык тупогуб, тупогубенький бычок, у быка бела губа была тупа.",
        "difficulty": "hard",
        "tags": ["животные"],
        "length": 58
      },
      {
        "id": 7,
        "text": "Корабли лавировали, лавировали, да не вылавировали.",
        "difficulty": "medium",
        "tags": ["море"],
        "length": 48
      },
      {
        "id": 8,
        "text": "Цапля чахла, цапля сохла, цапля сдохла.",
        "difficulty": "easy",
        "tags": ["животные", "грустное"],
        "length": 36
      },
      {
        "id": 9,
        "text": "Рапортовал, да не дорапортовал, а стал дорапортовывать — зарапортовался.",
        "difficulty": "hard",
        "tags": ["работа"],
        "length": 72
      },
      {
        "id": 10,
        "text": "Водовоз вёз воду из водопровода.",
        "difficulty": "easy",
        "tags": ["профессии"],
        "length": 30
      },
      {
        "id": 11,
        "text": "Кукушка кукушонку купила капюшон, кукушонок в капюшоне смешон.",
        "difficulty": "medium",
        "tags": ["животные", "одежда"],
        "length": 62
      },
      {
        "id": 12,
        "text": "Тридцать три корабля лавировали, лавировали, да не вылавировали.",
        "difficulty": "hard",
        "tags": ["море", "числа"],
        "length": 62
      },
      {
        "id": 13,
        "text": "Жужжит жужелица, жужжит, да не кружится.",
        "difficulty": "medium",
        "tags": ["насекомые"],
        "length": 40
      },
      {
        "id": 14,
        "text": "Полили ли лилию, видели ли Лидию?",
        "difficulty": "medium",
        "tags": ["цветы", "имена"],
        "length": 32
      },
      {
        "id": 15,
        "text": "От топота копыт пыль по полю летит.",
        "difficulty": "easy",
        "tags": ["лошади", "природа"],
        "length": 32
      },
      {
        "id": 16,
        "text": "Шестнадцать шушенских шерстобитов шерстобитили шерсть.",
        "difficulty": "hard",
        "tags": ["профессии", "числа"],
        "length": 52
      },
      {
        "id": 17,
        "text": "Фараонов фаворит на сапфир сменял нефрит.",
        "difficulty": "hard",
        "tags": ["история", "драгоценности"],
        "length": 42
      },
      {
        "id": 18,
        "text": "Регулировщик лигуриец регулировал в Лигурии.",
        "difficulty": "hard",
        "tags": ["география"],
        "length": 46
      },
      {
        "id": 19,
        "text": "Мы ели, ели ершей у ели, их еле-еле у ели доели.",
        "difficulty": "medium",
        "tags": ["рыба", "елка"],
        "length": 48
      },
      {
        "id": 20,
        "text": "Сеня вёз воз сена, Сеня спал на сене.",
        "difficulty": "easy",
        "tags": ["имя", "сельское"],
        "length": 36
      },
      {
        "id": 21,
        "text": "Щипцы да клещи — вот наши вещи.",
        "difficulty": "easy",
        "tags": ["инструменты"],
        "length": 28
      },
      {
        "id": 22,
        "text": "У перепела и перепёлки пять перепелят.",
        "difficulty": "medium",
        "tags": ["птицы", "семья"],
        "length": 38
      },
      {
        "id": 23,
        "text": "Стоит поп на копне, колпак на попе, копна под попом, поп под колпаком.",
        "difficulty": "hard",
        "tags": ["религия", "юмор"],
        "length": 68
      },
      {
        "id": 24,
        "text": "Все бобры для своих бобрят добры.",
        "difficulty": "easy",
        "tags": ["животные", "семья"],
        "length": 32
      },
      {
        "id": 25,
        "text": "Ткет ткач ткани на платки Тане.",
        "difficulty": "medium",
        "tags": ["профессии", "имя"],
        "length": 30
      },
      {
        "id": 26,
        "text": "Косарь Касьян косой косит косо — не скосит Касьян покоса.",
        "difficulty": "hard",
        "tags": ["имя", "работа"],
        "length": 56
      },
      {
        "id": 27,
        "text": "Пётр Петрович, по прозванью Перов, поймал птицу пигалицу.",
        "difficulty": "medium",
        "tags": ["имя", "птицы"],
        "length": 56
      },
      {
        "id": 28,
        "text": "Шла Саша по шоссе и сосала сушку, да ещё и чашку с чаем поставила на шкаф.",
        "difficulty": "hard",
        "tags": ["классика", "дополненная"],
        "length": 74
      },
      {
        "id": 29,
        "text": "У ёлки иголки колки.",
        "difficulty": "easy",
        "tags": ["елка"],
        "length": 20
      },
      {
        "id": 30,
        "text": "Везёт Сенька Саньку с Сонькой на санках.",
        "difficulty": "medium",
        "tags": ["имя", "зима"],
        "length": 40
      },
      {
        "id": 31,
        "text": "Клара у Карла украла кларнет, а Карл у Клары украл квитанцию.",
        "difficulty": "hard",
        "tags": ["классика", "дополненная"],
        "length": 62
      },
      {
        "id": 32,
        "text": "Скороговорун скороговорил, выскороговаривал, что все скороговорки не перескороговоришь, не перевыскороговоришь.",
        "difficulty": "hard",
        "tags": ["мета"],
        "length": 108
      },
      {
        "id": 33,
        "text": "Жил-был жирный жук, женился на пуме.",
        "difficulty": "medium",
        "tags": ["животные", "юмор"],
        "length": 36
      },
      {
        "id": 34,
        "text": "Хитрую сороку поймать морока, а сорок сорок — сорок морок.",
        "difficulty": "hard",
        "tags": ["птицы", "числа"],
        "length": 60
      },
      {
        "id": 35,
        "text": "Лёша на лавочке нашёл копеечку.",
        "difficulty": "easy",
        "tags": ["имя", "деньги"],
        "length": 32
      },
      {
        "id": 36,
        "text": "Купи кипу пик, кипу пик купи.",
        "difficulty": "medium",
        "tags": ["повтор"],
        "length": 28
      },
      {
        "id": 37,
        "text": "Мамаша Ромаше дала сыворотку из-под простокваши.",
        "difficulty": "medium",
        "tags": ["семья", "еда"],
        "length": 50
      },
      {
        "id": 38,
        "text": "Еле-еле Лена ела, есть из лени не хотела.",
        "difficulty": "easy",
        "tags": ["имя", "лень"],
        "length": 44
      },
      {
        "id": 39,
        "text": "Краснокожие креветки кокетливо кокнули в кокосовые кокошники.",
        "difficulty": "hard",
        "tags": ["еда", "цвета"],
        "length": 66
      },
      {
        "id": 40,
        "text": "Проворонила ворона воронёнка.",
        "difficulty": "medium",
        "tags": ["птицы"],
        "length": 30
      },
      {
        "id": 41,
        "text": "Три дроворуба на трёх дворах дрова рубят.",
        "difficulty": "hard",
        "tags": ["профессии", "числа"],
        "length": 44
      },
      {
        "id": 42,
        "text": "У осы не усы, не усищи, а усики.",
        "difficulty": "easy",
        "tags": ["насекомые"],
        "length": 32
      },
      {
        "id": 43,
        "text": "Ёжик у ежа, уж у ужа.",
        "difficulty": "easy",
        "tags": ["животные"],
        "length": 22
      },
      {
        "id": 44,
        "text": "Полина пела, пела, да и распелась.",
        "difficulty": "easy",
        "tags": ["имя", "музыка"],
        "length": 38
      },
      {
        "id": 45,
        "text": "Бублик, баранку, батон и буханку пекарь из теста испёк спозаранку.",
        "difficulty": "medium",
        "tags": ["еда", "профессии"],
        "length": 68
      },
      {
        "id": 46,
        "text": "Кот катался на клубке, клубок катался на коте.",
        "difficulty": "easy",
        "tags": ["животные", "юмор"],
        "length": 50
      },
      {
        "id": 47,
        "text": "У четырёх черепашек по четыре черепашонка.",
        "difficulty": "medium",
        "tags": ["животные", "числа"],
        "length": 46
      },
      {
        "id": 48,
        "text": "Стоял стол столом, на столе стакан с водой.",
        "difficulty": "easy",
        "tags": ["предметы"],
        "length": 44
      },
      {
        "id": 49,
        "text": "Фёдор топором рубит дрова, дрова рубят Фёдора топором.",
        "difficulty": "hard",
        "tags": ["имя", "инструменты"],
        "length": 56
      },
      {
        "id": 50,
        "text": "Шапка да шубка — вот и весь Мишутка.",
        "difficulty": "easy",
        "tags": ["одежда", "медведь"],
        "length": 36
      },
      {
        "id": 51,
        "text": "Криворукий кракодил крабовые крокодилы крошил в кратер Килиманджаро.",
        "difficulty": "hard",
        "tags": ["животные", "абсурд"],
        "length": 70
      },
      {
        "id": 52,
        "text": "Гипнотизёр гипнотизировал гиппопотама, гиппопотам хлопал гипнотизёра по темени.",
